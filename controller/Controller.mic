				decTimer, pulldownCLK, br (if timerIsNot0 then this)
				wrData, srDataAndIncBitsOnFallingEdge, br (if bitsIsNot11 then this)
				clBits
readStart:		srDataAndIncBitsOnFallingEdge, br (if clkNotFalling then this)
				br (if acked then readPackets)
				br (if bitsIsNot11 then readStart)
				clBits, br (if dataIsNotFA then readStart)
				stAcked, br readStart
readPackets:	br (if bitsIsNot33 then readStart)
				ldXYStatus, clBits, br readStart
