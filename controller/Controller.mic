				incTimer, pulldownCLK, br (if timerIsNot5050 then this)
				sendData, srDataAndIncBitsOnFallingEdge, br (if bitsIsNot11 then this)
				clBits
readStart:		br (if clkNotFalling then this)
				srData, br (if acked then readPackets)
				br (if bitsIsNot11 then readStart)
				clBits, br (if dataIsNotFA then readStart)
				stAcked, br readStart
readPackets:	br (if bitsIsNot33 then readStart)
				ldX, ldY, ldStatus, clBits, br readStart
